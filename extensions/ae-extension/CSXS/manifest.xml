<?xml version="1.0" encoding="UTF-8"?>
<ExtensionManifest xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" Version="7.0" ExtensionBundleId="com.sync.extension.ae" ExtensionBundleVersion="0.8.30" ExtensionBundleName="sync. extension - After Effects">
  <ExtensionList>
    <Extension Id="com.sync.extension.ae" Version="0.8.30"/>
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList>
      <Host Name="AEFT" Version="[24.0,99.9]"/>
    </HostList>
    <LocaleList>
      <Locale Code="All"/>
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="12.0"/>
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <Extension Id="com.sync.extension.ae" Version="0.8.30">
      <HostList>
        <Host Name="AEFT" Version="[24.0,99.9]"/>
      </HostList>
      <DispatchInfo>
        <Resources>
          <MainPath>./index.html</MainPath>
          <CEFCommandLine>
            <Parameter>--allow-file-access-from-files</Parameter>
            <Parameter>--allow-insecure-localhost</Parameter>
            <Parameter>--disable-features=BlockInsecurePrivateNetworkRequests,OutOfBlinkCors</Parameter>
            <Parameter>--enable-nodejs</Parameter>
            <Parameter>--enable-media-stream</Parameter>
            <Parameter>--use-fake-ui-for-media-stream</Parameter>
            <Parameter>--autoplay-policy=no-user-gesture-required</Parameter>
            <Parameter>--disable-web-security</Parameter>
          </CEFCommandLine>
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <Menu>sync. for After Effects</Menu>
          <Geometry>
            <Size>
              <Height>700</Height>
              <Width>480</Width>
            </Size>
            <MinSize>
              <Height>400</Height>
              <Width>400</Width>
            </MinSize>
            <MaxSize>
              <Height>2000</Height>
              <Width>2000</Width>
            </MaxSize>
          </Geometry>
          <Icons>
            <Icon Type="Normal">./icons/blue_icon.png</Icon>
            <Icon Type="RollOver">./icons/blue_icon.png</Icon>
            <Icon Type="Disabled">./icons/blue_icon.png</Icon>
            <Icon Type="DarkNormal">./icons/blue_icon.png</Icon>
            <Icon Type="DarkRollOver">./icons/blue_icon.png</Icon>
            <Icon Type="DarkDisabled">./icons/blue_icon.png</Icon>
          </Icons>
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>


