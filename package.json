{
  "name": "sync-extensions",
  "version": "0.9.60",
  "private": true,
  "type": "module",
  "workspaces": [
    "src/resolve",
    "src/server",
    "src/shared"
  ],
  "scripts": {
    "dev": "tsx scripts/dev.ts",
    "dev:server": "NODE_ENV=development HOST_APP=PPRO tsx src/server/server.ts",
    "dev:resolve": "concurrently -n vite,server -c blue,green --kill-others-on-fail \"vite\" \"NODE_ENV=development HOST_APP=RESOLVE tsx src/server/server.ts\"",
    "dev:resolve-build": "cross-env RESOLVE_BUILD=true NODE_ENV=development vite build --watch",
    "build": "npm run build:adobe && npm run build:davinci",
    "build:parallel": "concurrently -n adobe,davinci -c blue,green \"npm run build:adobe\" \"npm run build:davinci\"",
    "build:adobe": "npm run prezxp && npx dotenv-cli -e src/server/.env -- cross-env ZXP_PACKAGE=true NODE_ENV=production vite build",
    "build:davinci": "cross-env RESOLVE_BUILD=true RESOLVE_PACKAGE=true NODE_ENV=production vite build",
    "prezxp": "mkdir -p dist/cep/server && (grep -v '^ZXP_PASSWORD=' src/server/.env > dist/cep/server/.env 2>/dev/null || true)",
    "zxp": "npx dotenv-cli -e src/server/.env -- cross-env ZXP_PACKAGE=true vite build",
    "zip": "cross-env ZIP_PACKAGE=true vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "verify:zxp": "tsx scripts/verify-zxp.ts"
  },
  "dependencies": {
    "electron": "39.2.2",
    "lucide-react": "^0.554.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/fs-extra": "^11.0.4",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitejs/plugin-react": "^5.1.1",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "jsxbin": "^2.3.0",
    "rollup": "^4.53.2",
    "sass": "^1.94.1",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-cep-plugin": "^2.1.1"
  }
}
