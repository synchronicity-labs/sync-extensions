{
  "name": "sync-extensions",
  "version": "0.9.50",
  "private": true,
  "type": "module",
  "workspaces": [
    "src/resolve"
  ],
  "scripts": {
    "dev": "concurrently -n vite,server,resolve -c blue,green,yellow \"vite\" \"npm run dev:server\" \"npm run dev:resolve-build\"",
    "dev:server": "NODE_ENV=development HOST_APP=PPRO tsx src/server/server.ts",
    "dev:resolve": "concurrently -n vite,server -c blue,green \"vite\" \"NODE_ENV=development HOST_APP=RESOLVE tsx src/server/server.ts\"",
    "dev:resolve-build": "cross-env RESOLVE_BUILD=true NODE_ENV=development vite build --watch",
    "build": "npm run build:adobe && npm run build:davinci",
    "build:parallel": "concurrently -n adobe,davinci -c blue,green \"npm run build:adobe\" \"npm run build:davinci\"",
    "build:adobe": "npm run prezxp && dotenv -e src/server/.env -- cross-env ZXP_PACKAGE=true NODE_ENV=production vite build",
    "build:davinci": "cross-env RESOLVE_BUILD=true RESOLVE_PACKAGE=true NODE_ENV=production vite build",
    "install": "npm run build",
    "prezxp": "mkdir -p dist/cep/server && (grep -v '^ZXP_PASSWORD=' src/server/.env > dist/cep/server/.env 2>/dev/null || true) && (rm -f dist/cep/.debug dist/cep/cep/.debug 2>/dev/null || true)",
    "zxp": "dotenv -e src/server/.env -- cross-env ZXP_PACKAGE=true vite build",
    "zip": "cross-env ZIP_PACKAGE=true vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.908.0",
    "@aws-sdk/s3-request-presigner": "^3.908.0",
    "@elevenlabs/elevenlabs-js": "^2.19.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@xenova/transformers": "^2.16.1",
    "cors": "^2.8.5",
    "data-uri-to-buffer": "^6.0.1",
    "dotenv": "^17.2.3",
    "exiftool-vendored": "^25.0.0",
    "express": "^4.21.2",
    "fluent-ffmpeg": "^2.1.3",
    "form-data": "^4.0.0",
    "fs-extra": "^11.2.0",
    "lucide-react": "^0.553.0",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "posthog-node": "^5.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tesseract.js": "^5.0.6",
    "tmp": "^0.2.3",
    "tsx": "^4.7.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/fs-extra": "^11.0.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsxbin": "^2.1.3",
    "rollup": "^4.52.5",
    "sass": "^1.69.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-cep-plugin": "^1.2.9"
  }
}
